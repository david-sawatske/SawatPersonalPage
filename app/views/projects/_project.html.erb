<div class='project'>
  <h3><%= link_to project.title, project_path(project) %></h3>
  <%= link_to "Edit", edit_project_path(project) %>
  <%= link_to "Delete", project, method: :delete, data: { confirm: 'Are you sure?' } %>
  <p><%= project.description %></p>
  <%= image_tag project.image %>
</div>
